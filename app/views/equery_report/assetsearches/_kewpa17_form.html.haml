.row
  .col-md-10.col-md-offset-1
    %br
    / Nav tabs
    %ul.nav.nav-tabs
      %li.active= link_to (t 'equery.search_by'),"#search", "data-toggle" => "tab"

    .tab-content{style:"padding-top:1em"}
      #search.tab-pane.active
        .col-md-offset-3
          %br

          .form-group              
            .col-sm-2.control-label
              %b= t('equery.asset.disposalreport_for')
            .col-md-5
              .input-group
                = f.text_field :examine_start, class: 'form-control date_picker_reverse'    
                .input-group-addon=t('to')
                = f.text_field :examine_end, class: 'form-control date_picker_reverse'
