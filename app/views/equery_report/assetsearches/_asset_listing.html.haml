/ Nav tabs
%ul.nav.nav-tabs
  %li.active= link_to  (t 'equery.result'), "#search", "data-toggle" => "tab"

/ display 
.tab-content{style:"padding-top:1em"}
  #search_details.tab-pane.active  
  
    - if assetsearch.search_type==1
      = render partial: 'kewpa2n3_listing', locals: {assetsearch: assetsearch, assets: assets}
        
    - elsif assetsearch.search_type==2 || assetsearch.search_type==3
      = render partial: 'kewpa4n5_listing', locals: {assetsearch: assetsearch, assets: assets}
          
    - elsif assetsearch.search_type==4
      = render partial: 'kewpa6_listing', locals: {assetsearch: assetsearch, assets: assets}
          
    - elsif assetsearch.search_type==5
      = render partial: 'kewpa7_listing', locals: {assetsearch: assetsearch, assets: assets}
          
    - elsif assetsearch.search_type==6
      = render partial: 'kewpa8_listing', locals: {assetsearch: assetsearch, assets: assets}
        
    - elsif assetsearch.search_type==7
      = render partial: 'kewpa9_listing', locals: {assetsearch: assetsearch, assets: assets}
       
    - elsif assetsearch.search_type==8 || assetsearch.search_type==9
      = render partial: 'kewpa13n14_listing', locals: {assetsearch: assetsearch, assets: assets}
        
    - elsif assetsearch.search_type==10
      = render partial: 'kewpa16_listing', locals: {assetsearch: assetsearch, assets: assets}
          
    - elsif assetsearch.search_type==11
      = render partial: 'kewpa17_listing', locals: {assetsearch: assetsearch, assets: assets}
          
    - elsif assetsearch.search_type==12 || assetsearch.search_type==13
      = render partial: 'kewpa18n19_listing', locals: {assetsearch: assetsearch, assets: assets}
          
    - elsif assetsearch.search_type==14
      = render partial: 'kewpa20_listing', locals: {assetsearch: assetsearch, assets: assets}
          
    - elsif assetsearch.search_type==15
      /kewpa 28
          
    - elsif assetsearch.search_type==16
      /kewpa 30
          
    - elsif assetsearch.search_type==17
      /kewpa 31
     
        
        


/       .right{style:"padding: 0 10px; margin:0;"}= paginate assets, :theme => 'twitter-bootstrap-3', :pagination_class => 'pagination-sm'
        
         