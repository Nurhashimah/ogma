.row
  .col-md-10.col-md-offset-2
    - if @searchexamanalysistype=='1' || @searchexamanalysistype==1      
      /:programme_id, :examtype, :subject_id, :examon, :exampaper
    
      / Nav tabs
      %ul.nav.nav-tabs
        %li.active= link_to (t 'equery.search_by'),"#search", "data-toggle" => "tab"

      .tab-content{style:"padding-top:1em"}
        #search.tab-pane.active
          %br
          .col-md-offset-2
            .form-group
              .col-sm-2.control-label
                = f.label (t 'equery.examanalysis.programme_id') 
              .col-md-5
                = f.select :programme_details, options_for_select(Programme.roots.map(&:programme_list)), {:include_blank => (t 'select')}, {class: 'form-control mandatory'}

            .form-group
              .col-sm-2.control-label
                = f.label (t 'equery.examanalysis.subject_id')
              .col-md-5
                = f.select :subject_id, grouped_options_for_select(Programme.subject_groupbyprogramme2), {:include_blank => (t 'select')}, {class: 'form-control mandatory'}
              
            .form-group
              .col-sm-2.control-label
                = f.label  (t 'equery.examanalysis.examon')
              .col-md-2
                = f.text_field :examon, class: 'form-control date_picker_reverse'
