- model_class = Personalizetimetablesearch
.page-header
  %h1=t('equery.personalize_timetable.search')
= render 'shared/flashes'

.row
  .col-md-offset-1
    =render partial: 'personalizetimetable', locals: {personalizetimetablesearch: @personalizetimetablesearch, personalizetimetables: @personalizetimetables}

.row
  .col-md-6.col-md-offset-4
    %hr
    .form-actions
      /= link_to content_tag(:i, "", :class => "fa fa-arrow-left") +" #{t('equery.title')} - #{t('training.title')}", "/equery_reports?query_module=5", class: 'btn btn-primary'
      = link_to content_tag(:i, "", :class => "fa fa-arrow-left") +" #{t('equery.title')} - #{t('training.title')}", "/equery_reports/training", class: 'btn btn-primary'
      %span{style: 'padding-left: 20px;'}= link_to content_tag(:i, "", :class => "fa fa-arrow-left ") + " " + t('.back', :default => t("helpers.links.back")), new_equery_report_personalizetimetablesearch_path(searchpersonalizetimetabletype: 1),  :class => 'btn btn-default'
