- model_class = Tenant
.page-header
  %h1
    = t('campus.tenant.new')

.row
  = form_for [:campus, @tenant], :html => { :class => 'form-horizontal' }  do |f| 
    = render 'shared/error_messages', object: f.object
    .col-md-offset-1
      = render :partial => 'form', :locals => {:f => f}
      .form-group
        .col-sm-offset-2.col-sm-2
          = f.submit t('update'), class: "btn btn-primary"