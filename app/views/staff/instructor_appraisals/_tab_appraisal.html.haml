.col-md-10.col-md-offset-1
  %table.table.table-bordered
    %tr
      %th{:colspan=>"5"}=t 'exam.evaluate_course.evaluation_scale'
    %tr
      %td.centre{:width=>"80px;"} 2
      %td.centre{:width=>"80px;"} 1 
      %td.centre{:width=>"80px;"} 0
    %tr
      %td.centre=t 'instructor_appraisal.always_practice'
      %td.centre=t 'instructor_appraisal.frequently_practice'
      %td.centre=t 'instructor_appraisal.rarely_practice'

