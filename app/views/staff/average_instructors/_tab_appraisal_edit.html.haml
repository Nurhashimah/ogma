      %th.centre=((t 'average_instructor.weightage')+'<br> (w)').html_safe
      %th=((t 'average_instructor.percent')+'<br><u>y</u> X w<br>x').html_safe
    %tr
      %th A
      %td=t('average_instructor.preparation').upcase
      %td.centre 45
      %td.col-md-1=text_field_tag "total_part_a_summary"
      %td.centre 25
      %td.col-md-2=text_field_tag "total_part_a_percent"
    %tr
      %th B
      %td=t('average_instructor.delivery').upcase
      %td.centre 60
      %td.col-md-1=text_field_tag "total_part_b_summary"
      %td.centre 30
      %td.col-md-2=text_field_tag "total_part_b_percent"
    %tr
      %th C
      %td=t('average_instructor.usage').upcase
      %td.centre 20
      %td.col-md-1=text_field_tag "total_part_c_summary"
      %td.centre 5
      %td.col-md-2=text_field_tag "total_part_c_percent"
    %tr
      %th D
      %td=t('average_instructor.verification').upcase
      %td.centre 25
      %td.col-md-1=text_field_tag "total_part_d_summary"
      %td.centre 20
      %td.col-md-2=text_field_tag "total_part_d_percent"
    %tr
      %th E
      %td=t('average_instructor.general_teaching_technique').upcase
      %td.centre 45
      %td.col-md-1=text_field_tag "total_part_e_summary"
      %td.centre 20
      %td.col-md-2=text_field_tag "total_part_e_percent"

  %table.table.table-bordered.table-condensed
    %tr.warning
      %th.centre=t('average_instructor.score_range')
      %th.centre=t('average_instructor.grade')
      %th.centre=t('average_instructor.performance')
    %tr
      %td.centre 85 - 100
      %td.centre A
      %td.centre=t('average_instructor.excellent')
    %tr
      %td.centre 70 - 84
      %td.centre B
      %td.centre=t('average_instructor.good')
    %tr
      %td.centre 50 - 69
