%tr
  %th{:colspan=>6, style: 'text-align: center;'}= t 'student.attendance.class_schedule_details'
%tr
  %th No
  %th=t('student.attendance.subject')
  %th=t('student.attendance.topic')
  %th=t('student.attendance.date')
  %th=t('student.attendance.time')
  /%th=t('student.attendance.lecturer')
  /%td=t('student.attendance.intake2')
- @schedule_list.each_with_index do |schedule, ind|
  %tr
    %td=ind+=1
    %td=schedule.weeklytimetable_topic.full_parent
    %td=schedule.weeklytimetable_topic.subject_list
    %td=schedule.get_date_day_of_schedule+" "
    %td=schedule.get_time_slot
    /%td=schedule.weeklytimetable_lecturer.staff_with_rank
    /%td=schedule.weeklytimetable.schedule_intake.name 

