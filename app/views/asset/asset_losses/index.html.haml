- model_class = AssetLoss
.page-header
  %h1= t('asset.loss')#, :default => model_class.model_name.human.pluralize
  
.navbar.navbar-inverse{role: "navigation", style: "padding-right:40px;"}
  %ul.nav.navbar-nav
    %li= link_to content_tag(:li, fa_icon("asterisk", text: (t 'actions.new'))), '#'
  %ul.nav.navbar-nav.navbar-right
    %li.toga= link_to content_tag(:li, fa_icon("search", text: (t 'actions.search'))), "#", class: "toga"
    %li.dropdown
      %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"}
        = fa_icon "print", text: (t 'actions.print')

.search{style: "display:none;"}
  //= render "index_search"

.row
  %table.table-striped.table-hover.ogma
    %thead
      %tr
        %th Lost Type	
        %th Estimated Value
        %th Asset Name
        %th Lost Location 
        %th Loss Date $ Time
        %th Treasury Approval Ref No
		
