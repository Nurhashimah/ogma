.col-md-10.col-md-offset-1
  %dl.dl-horizontal
    - if @asset_loan.is_endorsed == true
      %dt=t 'asset.loan.is_endorsed'
      %dd=h @asset_loan.is_endorsed? ? "#{(t 'asset.loan.endorsed_date')} #{l(@asset_loan.endorsed_date) unless @asset_loan.endorsed_date.nil?} #{t('by2')}  #{@asset_loan.try(:loanofficer).try(:staff_with_rank)}" : "" 
    - else
      %dt Status
      %dd=t 'not_endorsed'
    
    %dt=t 'asset.loan.endorsed_note'
    %dd=h @asset_loan.endorsed_note
