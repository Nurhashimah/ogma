= search_form_for @search, url: loanables_asset_assets_path,  class: "form-inline" do |f|
  %tr.search_row{:style => "display: none;"}
    %td= f.text_field :assetcode_cont, size: 2,  :class => "form-control"
    %td= f.text_field :typename_cont, size: 8,  :class => "form-control"
    %td= f.text_field :name_cont, size: 8, :class => "form-control"
    %td= f.text_field :modelname_cont, size: 8, :class => "form-control"
    %td= f.text_field :serialno_cont, size: 8, :class => "form-control"
    %td 
      =f.text_field :assignedto_name_cont, placeholder: (t 'asset.loan.loaned_by'), class: 'form-control'
      =f.text_field :assignedto_positions_unit_cont, placeholder: (t 'asset.loan.responsible_unit'), class: 'form-control'
    %td
      %button.btn.btn-sm.btn-default.right{:type => "submit"}
        %i.fa.fa-search.fa-fw