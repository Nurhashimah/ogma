= search_form_for [:examresult, @search], url: exam_examresults_path,  class: "form-inline" do |f|
  %tr.search_row{style: "display: none;"}
    %td= f.text_field :programmestudent_name_or_programmestudent_course_type_or_programmestudent_level_cont, class: 'form-control'
    %td= f.text_field :examdts_gteq, class: 'form-control date_picker', size: 2
    - if current_user.college.code=='kskbjb'
      %td= f.text_field :examdte_gteq, class: 'form-control date_picker', size: 2
      %td
        .input-group
          = select_tag('q[semester_eq]',options_for_select([[t('all_records'),'']]+DropDown::SEMESTER),class: 'selectpicker',"data-width"=>'100px')
          %span.input-group-btn
            %button.btn.btn-sm.btn-default.right{:type => "submit"}
              %i.fa.fa-search.fa-fw
    - else
      %td 
        .input-group
          = f.text_field :examdte_gteq, class: 'form-control date_picker', size: 2
          %span.input-group-btn
            %button.btn.btn-sm.btn-default.right{:type => "submit"}
              %i.fa.fa-search.fa-fw